{
  "name": "algoext",
  "version": "1.0.0",
  "description": "Extension for quick search for algorithms",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "watch:p": "cross-env NODE_ENV=production webpack --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack",
    "dist": "gh-pages -d dist",
    "eslint": "npx eslint --fix src/.",
    "stats": "npx webpack --mode production --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslannn199/algoext.git"
  },
  "browserslist": [
    "last 2 version",
    "> 0.5%",
    "IE 10"
  ],
  "keywords": [
    "algorithms",
    "data",
    "structures",
    "javascript",
    "typescript"
  ],
  "author": "Ruslan Kamenskiy <ruslannn199@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ruslannn199/algoext#readme",
  "bugs": {
    "url": "https://github.com/ruslannn199/algoext/issues"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "browserslist": "^4.21.7",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.41.0",
    "gh-pages": "^5.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "postcss-preset-env": "^8.4.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.js"
    }
  }
}
